@startuml

start
  :cur指针指向根节点;

repeat

if (左子树为空?) then (是)
    if(右子树为空?) then (是)
        :输出当前元素;
        if(栈为空?) then (是)
            stop
        else (否)
            :栈顶元素出栈并赋值给当前元素;
            if (右子树为空?) then(是)
                :置空左子树;
            else (否)
                :输出当前元素;
                :指向右子树
                cur = cur.right;
            endif
        endif
    else (否)
        :输出当前元素;
        :指向右子树
        cur = cur.right;
    endif
else (否)
    :入栈当前元素;
    :指向左子树
    cur = cur.left;
endif

@enduml